@model VacationManager.WEB.Models.MonthViewModel
<table cellpadding="0" cellspacing="0">
    @for (int i = 0; i != this.Model.Days.GetLength(0); i++)
    {
        <tr>
            @for (int j = 0; j != this.Model.Days.GetLength(1); j++)
            {
                <td>
                    @Html.Partial("_Day", this.Model.Days[i, j])
                </td>
            }
        </tr>
    }
</table>
<div class="ch-month-label">@Model.Date.ToString("MMM  \\'yy")</div>
